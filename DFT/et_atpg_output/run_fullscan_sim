# Template script for running Cadence Incisive simulator (NCSIM) on Cadence Encounter Test generated test vectors.
 
#  Generated by Genus(TM) Synthesis Solution 16.13-s036_1
 
export WORKDIR=./et_atpg_output
 
irun \
   +nospecify \
   +access+rwc \
   +ncstatus \
   +nc64bit \
   +TESTFILE1=$WORKDIR/testresults/verilog/VER.FULLSCAN.TOP_atpg.data.scan.ex1.ts1.verilog \
   +TESTFILE2=$WORKDIR/testresults/verilog/VER.FULLSCAN.TOP_atpg.data.logic.ex1.ts2.verilog \
   +HEARTBEAT \
   +FAILSET \
   +nctimescale+1ns/1ps \
   +ncoverride_timescale \
   +ncseq_udp_delay+2ps \
   +libext+.v+.V+.z+.Z+.gz \
   +nclibdirname+$WORKDIR/Inca_libs_12_28_26 \
   -l $WORKDIR/ncverilog_FULLSCAN.log \
  -v /home/Team14/library/fast_vdd1v0_basicCells.v \
$WORKDIR/TOP.et_netlist.v \
$WORKDIR/testresults/verilog/VER.FULLSCAN.TOP_atpg.mainsim.v

